{
  "options": {
    "outFile": "dist/wasm.wasm",
    "textFile": "dist/wasm.wat",
    "stats": true,
    "runtime": "stub",
    "exportRuntime": true,
    "bindings": "esm",
    "enable": ["threads", "simd"]
  },
  "targets": {
    "development": {
      "sourceMap": true,
      "debug": true
    },
    "production": {
      "sourceMap": true,
      "optimizeLevel": 3,
      "shrinkLevel": 1,
      "converge": true,
      "noAssert": false
    }
  }
}
