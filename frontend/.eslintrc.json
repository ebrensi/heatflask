{
    "env": {
      "browser": true,
      "worker": true,
      "es2022": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "prettier"
    ],
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "dot-notation": "off"
    },
    "ignorePatterns": [
      "dist/*",
      "src/dist/*"
    ]
}
