{
  "addons": [
    "papertrail",
    "heroku-postgresql",
    "redisgreen"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    }
  ],
  "env": {
    "APP_SETTINGS": {
      "required": true
    },
    "ATLAS_MONGODB_URI": {
      "required": true
    },
    "FLASK_APP": {
      "required": true
    },
    "IPSTACK_ACCESS_KEY": {
      "required": true
    },
    "PAPERTRAIL_API_TOKEN": {
      "required": true
    },
    "PAYPAL_CLIENT_ID": {
      "required": true
    },
    "PAYPAL_CLIENT_SECRET": {
      "required": true
    },
    "REDISGREEN_URL": {
      "required": true
    },
    "STRAVA_CLIENT_ID": {
      "required": true
    },
    "STRAVA_CLIENT_SECRET": {
      "required": true
    }
  },
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "name": "heatmappp",
  "scripts": {
  },
  "stack": "heroku-18"
}
